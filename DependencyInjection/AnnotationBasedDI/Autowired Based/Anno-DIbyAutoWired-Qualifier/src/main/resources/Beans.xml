<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config> </context:annotation-config>

    <bean name="address1" class="com.wolters.Anno_DIbyAutoWired_Qualifier.pojos.Address">
        <property name="city" value="Mumbai"/>
        <property name="country" value="India"/>
    </bean>

    <bean name="address2" class="com.wolters.Anno_DIbyAutoWired_Qualifier.pojos.Address">
        <property name="city" value="Pune"/>
        <property name="country" value="India"/>
    </bean>

    <bean name="emp1" class="com.wolters.Anno_DIbyAutoWired_Qualifier.pojos.Employee">
        <property name="name" value="Ratnakar Patil"/>
    </bean>

</beans>
